    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

    <li>MemberShip TypeName : @Model.Name </li>
    @if (@Model.BirthdayDate != null)
    {
        <li>Birthday : @Model.BirthdayDate.Value.ToString("yyyy/MM/dd")</li>
    }

